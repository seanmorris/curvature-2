{
	plugins:   [
		'@babel/plugin-proposal-json-strings',
		'@babel/plugin-proposal-class-properties',
		'@babel/plugin-transform-modules-commonjs',
	]
	, presets: [
		[ '@babel/preset-env', {
			targets: {browsers: ['>0.25%',  'not ie 11', 'not op_mini all', 'not dead']},
			debug: true
		}],
	]
	, env: {production: {
		compact: true,
		comments: false,
		presets: [
			[ 'minify' , {
				builtIns: false,
				mergeVars: false,
			}]
		]
	}}
}
